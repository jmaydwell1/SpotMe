<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StickerSelect">

    <ImageView
        android:id="@+id/imgfacebook"
        android:layout_width="91dp"
        android:layout_height="92dp"
        android:contentDescription="@string/facebook"
        android:src="@drawable/facebook"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/imgyoutube"
        app:layout_constraintEnd_toStartOf="@id/imginstagram"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/imginstagram"
        android:layout_width="91dp"
        android:layout_height="92dp"
        android:contentDescription="@string/imginstagram"
        android:src="@drawable/instagram"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/imgfacebook"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <ImageView
        android:id="@+id/imgyoutube"
        android:layout_width="91dp"
        android:layout_height="92dp"
        android:contentDescription="@string/youtube"
        android:src="@drawable/youtube"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/imgfacebook"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>